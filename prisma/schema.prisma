// prisma/schema.prisma
// This is your Prisma schema file, defines data models and database connection

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id_Usuario Int    @id @default(autoincrement())
  cedula     String @unique
  nombre     String
  apellidos  String
  email      String @unique
  telefono   Int
}